{
  "name": "knifes-shop",
  "private": true,
  "scripts": {
    "build:libs": "turbo run build:libs",
    "build": "turbo run build",
    "dev": "npm run build:libs && turbo run dev --filter=web --filter=api",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "class-validator": "^0.14.2",
    "zod": "^3.25.67"
  }
}
